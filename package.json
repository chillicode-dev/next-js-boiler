{
  "name": "next-js-boiler",
  "version": "2.0.0",
  "author": "Chillicode <info@chillicode.ru> (https://github.com/chillicode-dev)",
  "scripts": {
    "dev": "npm-run-all -p app:dev storybook:dev",
    "app:dev": "next dev -p 3000",
    "app:build": "next build",
    "app:export": "next export",
    "app:start": "next start",
    "storybook:dev": "start-storybook -p 3001 --ci --quiet --static-dir ./public",
    "storybook:build": "build-storybook",
    "eslint:inspect": "eslint \"./**/*.{js,jsx}\" --ignore-path .gitignore .",
    "eslint:fix": "npm run eslint:inspect -- --fix",
    "stylelint:inspect": "stylelint \"./**/*.{css,scss}\" \"!./styles/vendor/**\" --ignore-path .gitignore",
    "stylelint:fix": "npm run stylelint:inspect -- --fix",
    "prettier:inspect": "prettier --check \"./**/*.{js,jsx,json,css,scss}\" --ignore-path .gitignore",
    "prettier:fix": "prettier --write \"./**/*.{js,jsx,json,css,scss}\" --ignore-path .gitignore",
    "linters:inspect": "npm-run-all -p prettier:inspect stylelint:inspect eslint:inspect",
    "linters:fix": "npm-run-all -p prettier:fix stylelint:fix eslint:fix"
  },
  "dependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-decorators": "^7.12.13",
    "@storybook/addon-actions": "^6.1.14",
    "@storybook/addon-essentials": "^6.1.14",
    "@storybook/addon-links": "^6.1.14",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.1.14",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "classnames": "^2.2.6",
    "css-loader": "^5.0.1",
    "csso-webpack-plugin": "^2.0.0-beta.1",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "is-in-browser": "^1.1.3",
    "mobx": "^6.1.8",
    "mobx-react": "^7.1.0",
    "mobx-state-tree": "^5.0.1",
    "next": "10.0.5",
    "node-sass-json-importer": "^4.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "prop-types": "^15.7.2",
    "query-string": "^6.13.8",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-redux": "^7.2.2",
    "react-svg-loader": "^3.0.3",
    "sass": "^1.32.4",
    "sass-loader": "^10.1.1",
    "style-loader": "^2.0.0",
    "stylelint": "^15.10.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-scss": "^3.18.0"
  }
}
