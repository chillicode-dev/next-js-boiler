{
  "name": "react-boilerplate",
  "version": "2.0.0-rc.1",
  "private": true,
  "scripts": {
    "dev": "npm-run-all -p app:dev storybook:dev",
    "app:dev": "next dev",
    "app:build": "next build",
    "app:export": "next export",
    "app:start": "next start",
    "storybook:dev": "start-storybook -p 3001 --static-dir ./public",
    "storybook:build": "build-storybook",
    "prettier:inspect": "prettier --check \"./**/*.{js,jsx,json,css,scss}\"",
    "prettier:fix": "prettier --write \"./**/*.{js,jsx,json,css,scss}\"",
    "lint:inspect": "eslint \"./**/*.{js,jsx}\"",
    "lint:fix": "eslint \"./**/*.{js,jsx}\" --fix",
    "linters:inspect": "npm run prettier:inspect && npm run lint:inspect",
    "linters:fix": "npm run prettier:fix && npm run lint:fix",
    "stylelint:inspect": "stylelint \"./**/*.{css,scss}\" --syntax scss",
    "stylelint:fix": "stylelint \"./**/*.{css,scss}\" --syntax scss --fix"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "csso-webpack-plugin": "^2.0.0-beta.1",
    "is-in-browser": "^1.1.3",
    "next": "10.0.5",
    "node-sass-json-importer": "^4.3.0",
    "prop-types": "^15.7.2",
    "query-string": "^6.13.8",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@storybook/addon-actions": "^6.1.14",
    "@storybook/addon-essentials": "^6.1.14",
    "@storybook/addon-links": "^6.1.14",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.1.14",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.0.1",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.22.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "react-svg-loader": "^3.0.3",
    "sass": "^1.32.4",
    "sass-loader": "^10.1.1",
    "style-loader": "^2.0.0",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-scss": "^3.18.0"
  }
}
